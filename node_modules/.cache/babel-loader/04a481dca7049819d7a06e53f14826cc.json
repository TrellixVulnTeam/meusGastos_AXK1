A,GAAG,EAAC,GAAqD;AAAjD,IAAA,IAAI,EAAC,MAA4C;AAArC,IAAA,KAAK,EAAC,cAA+B;;aAAP,KAAA,CAAA,KAAA,GAAK,M;;AAAE,GAA9E,E,IAAA,E,GAAA,C,iBAAuE,KAAA,CAAA,K,GACrE,CAHN,CAQE,EAJF,YAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFF,WAEE,E,gBADF,YAAA,CAAkE,OAAlE,EAAkE;AAA3D,IAAA,IAAI,EAAC,MAAsD;AAA/C,IAAA,KAAK,EAAC,cAAyC;AAA1B,IAAA,EAAE,EAAC,MAAuB;;aAAP,KAAA,CAAA,IAAA,GAAI,M;;AAAG,GAAlE,E,IAAA,E,GAAA,C,iBAA2D,KAAA,CAAA,I,GACzD,CAHN,CAIE,CATN,CAgBE,EALF,YAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFF,WAEE,E,gBADF,YAAA,CAA0E,UAA1E,EAA0E;AAAhE,IAAA,GAAG,EAAC,GAA4D;AAAzD,IAAA,KAAK,EAAC,cAAmD;AAApC,IAAA,EAAE,EAAC,WAAiC;;aAAZ,KAAA,CAAA,SAAA,GAAS,M;;AAAG,GAA1E,E,IAAA,E,GAAA,C,iBAA8D,KAAA,CAAA,S,GAC5D,CAHN,CAKE,EADF,YAAA,CAAqE,QAArE,EAAqE;AAA5D,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,YAAA,IAAA,QAAA,CAAA,YAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAAuD;AAAvC,IAAA,KAAK,EAAC;AAAiC,GAArE,EAAsD,QAAtD,CACE,CA/BN,CAgCE,CAjCN,CAmCE,CApCN,CA0CE,G,kBAJF,YAAA,CAGM,SAHN,EAGM,IAHN,EAGM,WAAA,CAHc,IAAA,CAAA,gBAGd,EAH8B,UAAxB,IAAwB,EAApB;yBAAhB,YAAA,CAGM,KAHN,EAGM;AAHiC,MAAA,GAAG,EAAE,IAAI,CAAC;AAG3C,KAHN,E,CACI,YAAA,CAC+B,sBAD/B,EAC+B;AADjB,MAAA,UAAU,EAAE,IACK;AAA9B,MAAA,YAAU,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,eAAE,QAAA,CAAA,MAAA,CAAO,MAAP,CAAF;AAAA,OAAA;AAAoB,KAD/B,E,IAAA,E,CAAA,E,cAAA,C,CADJ,C;GAGM,CAHN,E,GAAA,CAIE,EA3CN,C","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"card\">\r\n            <div id=\"card-body\">\r\n                <div class=\"p-4\">\r\n                    <div id=\"radio\"> \r\n                        <div class=\"form-check\">\r\n                            <input class=\"form-check-input\" type=\"radio\" name=\"entrada\" id=\"entrada\" value=\"entrada\" v-model=\"tipo\" @click=\"selecionaEntrada\">\r\n                            <label class=\"form-check-label verde\" for=\"entada\" id=\"entrada\">\r\n                                <b>Entrada</b>\r\n                            </label>\r\n                        </div>\r\n                        <div class=\"form-check mx-3\">\r\n                            <input class=\"form-check-input\" type=\"radio\" name=\"saida\" id=\"saida\" value=\"saida\" v-model=\"tipo\" @click=\"selecionaSaida\">\r\n                            <label class=\"form-check-label vermelho\" for=\"saida\" id=\"saida\">\r\n                                <b>Saída</b>\r\n                            </label>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"d-flex w-100\">\r\n                        <div class=\"form-group\">\r\n                            <label for=\"valor\">Valor</label>\r\n                            <input type=\"number\" min=\"0\" step=\"0.01\" class=\"form-control\" v-model=\"valor\">\r\n                        </div>\r\n                        <div class=\"form-group mx-5\">\r\n                            <label for=\"valor\">Data</label>\r\n                            <input type=\"date\" class=\"form-control\" id=\"date\" v-model='data'/>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <div class=\"form-group w-100\">\r\n                        <label for=\"textarea\">Descrição</label>\r\n                        <textarea row=3  class=\"form-control\" id=\"descricao\" v-model='descricao'/>\r\n                    </div>\r\n                    <button @click=\"adicionaItem\" class=\"btn btn-primary\">Lançar</button>\r\n                </div>\r\n            </div>\r\n\r\n        </div>\r\n\r\n        <div v-for=\"item in todosLancamentos\" :key=\"item.id\">\r\n            <ItemLancado :lancamento=\"item\"\r\n            @removeItem=\"remove($event)\" />\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\";\r\nimport ItemLancado from \"./ItemLancado\"\r\nimport Lancamentos from '../models/Lancamentos'\r\n\r\nexport default {\r\n    name: \"PainelLancamento\",\r\n    data(){\r\n        return {\r\n            data: '2021-04-26',\r\n            tipo: 'saida',\r\n            valor: '',\r\n            descricao: '',\r\n        }\r\n\r\n    },\r\n    components:{\r\n        ItemLancado\r\n    },\r\n    methods:{\r\n        ...mapActions(['salvarLancamento','atualizaCaixa']),\r\n        adicionaItem(){\r\n\r\n            if(!this.valor || !this.data || !this.descricao){\r\n                if(!this.valor)alert('Informe o valor a ser lançado');\r\n                if(!this.data)alert('Informe a data do lançamento');\r\n                if(!this.descricao)alert('Informe uma descrição para o lançamento');\r\n                return;\r\n            }\r\n\r\n            let data = new Date(this.data);\r\n            let valor1 = parseFloat(this.valor);\r\n\r\n            if(this.tipo == 'saida'){\r\n                valor1 = valor1 - (valor1*2);\r\n            }\r\n\r\n            data = data.getDate() + '/' + (data.getMonth()+1) + '/' + data.getFullYear();\r\n\r\n            const lancamento = new Lancamentos(\r\n                this.tipo,\r\n                valor1,\r\n                data,\r\n                this.descricao, \r\n                new Date().getTime()\r\n            );\r\n\r\n            this.salvarLancamento(lancamento);\r\n            this.atualizaCaixa();\r\n            \r\n            this.valor = '';\r\n            this.descricao = '';\r\n\r\n        },\r\n        remove($event){\r\n            let id = $event.id\r\n            this.lancamentos = this.lancamentos.filter(hit =>{\r\n                return hit.id != id\r\n            });\r\n        },\r\n        selecionaEntrada(){\r\n            this.entrada = 'entrada';\r\n        },\r\n        selecionaSaida(){\r\n            this.entrada = 'saida';\r\n        }\r\n    },\r\n    computed: mapGetters(['todosLancamentos']),\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    #radio{\r\n        display: flex;\r\n    }\r\n    button{\r\n        margin: 10px;\r\n    }\r\n\r\n</style>"],"sourceRoot":""}]}