AD,CAAL;AACnB;AACJ;;AAEA,UAAI,IAAG,GAAI,IAAI,IAAJ,CAAS,KAAK,IAAd,CAAX;AACA,UAAI,MAAK,GAAI,UAAU,CAAC,KAAK,KAAN,CAAvB;;AAEA,UAAG,KAAK,IAAL,IAAa,OAAhB,EAAwB;AACpB,QAAA,MAAK,GAAI,MAAK,GAAK,MAAM,GAAC,CAA1B;AACJ;;AAEA,MAAA,IAAG,GAAI,IAAI,CAAC,OAAL,KAAiB,GAAjB,IAAwB,IAAI,CAAC,QAAL,KAAgB,CAAxC,IAA6C,GAA7C,GAAmD,IAAI,CAAC,WAAL,EAA1D;AAEA,UAAM,UAAS,GAAI,IAAI,WAAJ,CACf,KAAK,IADU,EAEf,MAFe,EAGf,IAHe,EAIf,KAAK,SAJU,EAKf,IAAI,IAAJ,GAAW,OAAX,EALe,CAAnB;AAQA,WAAK,gBAAL,CAAsB,UAAtB;AACA,WAAK,aAAL;AAEA,WAAK,KAAL,GAAa,EAAb;AACA,WAAK,SAAL,GAAiB,EAAjB;AAEH,KAlCE;AAmCH,IAAA,MAnCG,kBAmCI,MAnCJ,EAmCW;AACV,UAAI,EAAC,GAAI,MAAM,CAAC,EAAhB;AACA,WAAK,WAAL,GAAmB,KAAK,WAAL,CAAiB,MAAjB,CAAwB,UAAA,GAAE,EAAI;AAC7C,eAAO,GAAG,CAAC,EAAJ,IAAU,EAAjB;AACH,OAFkB,CAAnB;AAGH,KAxCE;AAyCH,IAAA,gBAzCG,8BAyCe;AACd,WAAK,OAAL,GAAe,SAAf;AACH,KA3CE;AA4CH,IAAA,cA5CG,4BA4Ca;AACZ,WAAK,OAAL,GAAe,OAAf;AACJ;AA9CG,IAdI;AA8DX,EAAA,QAAQ,EAAE,UAAU,CAAC,CAAC,kBAAD,CAAD;AA9DT,CAAf","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"card\">\r\n            <div id=\"card-body\">\r\n                <div class=\"p-4\">\r\n                    <div id=\"radio\"> \r\n                        <div class=\"form-check\">\r\n                            <input class=\"form-check-input\" type=\"radio\" name=\"entrada\" id=\"entrada\" value=\"entrada\" v-model=\"tipo\" @click=\"selecionaEntrada\">\r\n                            <label class=\"form-check-label verde\" for=\"entada\" id=\"entrada\">\r\n                                <b>Entrada</b>\r\n                            </label>\r\n                        </div>\r\n                        <div class=\"form-check mx-3\">\r\n                            <input class=\"form-check-input\" type=\"radio\" name=\"saida\" id=\"saida\" value=\"saida\" v-model=\"tipo\" @click=\"selecionaSaida\">\r\n                            <label class=\"form-check-label vermelho\" for=\"saida\" id=\"saida\">\r\n                                <b>Saída</b>\r\n                            </label>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"d-flex w-100\">\r\n                        <div class=\"form-group\">\r\n                            <label for=\"valor\">Valor</label>\r\n                            <input type=\"number\" min=\"0\" step=\"0.01\" class=\"form-control\" v-model=\"valor\">\r\n                        </div>\r\n                        <div class=\"form-group mx-5\">\r\n                            <label for=\"valor\">Data</label>\r\n                            <input type=\"date\" class=\"form-control\" id=\"date\" v-model='data'/>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <div class=\"form-group w-100\">\r\n                        <label for=\"textarea\">Descrição</label>\r\n                        <textarea row=3  class=\"form-control\" id=\"descricao\" v-model='descricao'/>\r\n                    </div>\r\n                    <button @click=\"adicionaItem\" class=\"btn btn-primary\">Lançar</button>\r\n                </div>\r\n            </div>\r\n\r\n        </div>\r\n\r\n        <div v-for=\"item in todosLancamentos\" :key=\"item.id\">\r\n            <ItemLancado :lancamento=\"item\"\r\n            @removeItem=\"remove($event)\" />\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\";\r\nimport ItemLancado from \"./ItemLancado\"\r\nimport Lancamentos from '../models/Lancamentos'\r\n\r\nexport default {\r\n    name: \"PainelLancamento\",\r\n    data(){\r\n        return {\r\n            data: '2021-04-26',\r\n            tipo: 'saida',\r\n            valor: '',\r\n            descricao: '',\r\n        }\r\n\r\n    },\r\n    components:{\r\n        ItemLancado\r\n    },\r\n    methods:{\r\n        ...mapActions(['salvarLancamento','atualizaCaixa']),\r\n        adicionaItem(){\r\n\r\n            if(!this.valor || !this.data || !this.descricao){\r\n                if(!this.valor)alert('Informe o valor a ser lançado');\r\n                if(!this.data)alert('Informe a data do lançamento');\r\n                if(!this.descricao)alert('Informe uma descrição para o lançamento');\r\n                return;\r\n            }\r\n\r\n            let data = new Date(this.data);\r\n            let valor1 = parseFloat(this.valor);\r\n\r\n            if(this.tipo == 'saida'){\r\n                valor1 = valor1 - (valor1*2);\r\n            }\r\n\r\n            data = data.getDate() + '/' + (data.getMonth()+1) + '/' + data.getFullYear();\r\n\r\n            const lancamento = new Lancamentos(\r\n                this.tipo,\r\n                valor1,\r\n                data,\r\n                this.descricao, \r\n                new Date().getTime()\r\n            );\r\n\r\n            this.salvarLancamento(lancamento);\r\n            this.atualizaCaixa();\r\n            \r\n            this.valor = '';\r\n            this.descricao = '';\r\n\r\n        },\r\n        remove($event){\r\n            let id = $event.id\r\n            this.lancamentos = this.lancamentos.filter(hit =>{\r\n                return hit.id != id\r\n            });\r\n        },\r\n        selecionaEntrada(){\r\n            this.entrada = 'entrada';\r\n        },\r\n        selecionaSaida(){\r\n            this.entrada = 'saida';\r\n        }\r\n    },\r\n    computed: mapGetters(['todosLancamentos']),\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    #radio{\r\n        display: flex;\r\n    }\r\n    button{\r\n        margin: 10px;\r\n    }\r\n\r\n</style>"],"sourceRoot":""}]}